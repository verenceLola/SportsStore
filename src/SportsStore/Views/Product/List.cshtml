@model ProductsListViewModel

@foreach (var p in Model.Products)
{
    @await Html.PartialAsync("ProductSummary", p)
}

<div page-model="@Model.PagingInfo" page-action="List" page-classes-enabled="true" page-class="btn"
    page-class-normal="btn-default" page-url-category="@Model.currentCategory" page-class-selected="btn-primary" class="btn-group pull-right">
</div>